@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<table class="table"
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserList.FirstOrDefault<ServiceNotification.Models.User>().UserAccount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserList.FirstOrDefault<ServiceNotification.Models.User>().UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserList.FirstOrDefault<ServiceNotification.Models.User>().Email)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model.UserList)
        {
        <tr>
            <td>
                @user.UserAccount
            </td>
            <td>
                @user.UserName
            </td>
            <td>
                @user.Email
            </td>
        </tr>
        }
        
    </tbody>
</table>